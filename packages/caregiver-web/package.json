    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.20",
    "jsdom": "^27.4.0",
    "msw": "^2.12.7",
    "postcss": "^8.4.49",
