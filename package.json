{
  "name": "flynn-aac",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "bun run --filter './packages/*' dev",
    "build": "bun run --filter './packages/*' build",
    "test": "bun run --filter './packages/*' test",
    "lint": "bun run --filter './packages/*' lint",
    "typecheck": "bun run --filter './packages/*' typecheck",
    "clean": "rm -rf packages/*/node_modules packages/*/dist && rm -rf node_modules"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  }
}
